report_template unknown_docker {
  shortdoc: "Report BASIC command invocation info for docker commands"
  doc: """
Intended for reporting only basic identifying information for docker commands
that do not generate a chalk report.
"""
  key._OPERATION.use                          = true
  key._ACTION_ID.use                          = true
  key._DATETIME.use                           = true
  key._ARGV.use                               = true
  key._OP_CHALKER_COMMIT_ID.use               = true
  key._OP_CHALKER_VERSION.use                 = true
  key._OP_PLATFORM.use                        = true
  key._OP_HOSTINFO.use                        = true
  key._OP_NODENAME.use                        = true
}

report_template push_default {
  shortdoc: "Default report for docker push commands"
  doc: """
  When we report on a 'docker push', these are the important
  per-artifact items we collect. The `CURRENT_HASH` and `REPO_DIGESTS`
  fields are both necessary for following the breadcrumbs, so should be
  on in all circumstances (we'll probably force them on eventually).

  You can certainly collect a lot more data here, but we expect it
  should usually be redundant w/ the info you collected during a build.
"""
  key._ACTION_ID.use                          = true
  key._TIMESTAMP.use                          = true
  key._DATETIME.use                           = true
  key._OPERATION.use                          = true
  ~key._CURRENT_HASH.use                      = true
  key._STORE_URI.use                          = true
  key._REPO_TAGS.use                          = true
  ~key._REPO_DIGESTS.use                      = true
  key.INJECTOR_PUBLIC_KEY.use                 = true
  key.CHALK_ID.use                            = true
  key.METADATA_ID.use                         = true
}

report_template audit {
  shortdoc: "Default audit report (audits are off by default)"
  doc: """
  Here, we only report key bits about the configuration used, for audit
  purposes. This is implemented as the "audit" report below; the
  implementation special-cases it a bit; the "publish_audit" variable
  can be used to control this (right now, just turn it on or off, though
  you can certainly change this report, and change the output sink(s)
  subscribed to the 'audit' topic).

  Note that in the default configuration, "publish_audit" is OFF.
"""
  key._OPERATION.use                          = true
  key._ACTION_ID.use                          = true
  key._TIMESTAMP.use                          = true
  key._DATETIME.use                           = true
  key._OP_CHALK_COUNT.use                     = true
  key._OP_CMD_FLAGS.use                       = true
  key._OP_CONFIG.use                          = true
  key._CHALK_EXTERNAL_ACTION_AUDIT.use        = true
}

report_template usage {
  shortdoc: "Reports on summary statistics"
  doc: """
  These fields are, by default, sent back up to Crash Override to keep
  high-level statistics on Chalk usage (via an attempted post to an
  HTTPS endpoint).
"""
  ~key._OPERATION.use                         = true
  ~key._TIMESTAMP.use                         = true
  ~key._OP_CHALK_COUNT.use                    = true
  ~key._TENANT_ID.use                         = true
  ~key._OP_CHALKER_COMMIT_ID.use              = true
  ~key._OP_CHALKER_VERSION.use                = true
  ~key._OP_PLATFORM.use                       = true
}

